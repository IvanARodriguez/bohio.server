{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"ConnectionStrings": {
		"DefaultConnection": "Host=localhost;Port=5432;Database=Bohio_db;Username=postgres;Password=postgres;"
	},
	"JwtSettings": {
		"validIssuer": "bohio-server",
		"validAudience": "bohio-client"
	},
	"Serilog": {
		"Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		}
	}
}
